{
	"source": {
		"include": ["src"],
		"includePattern": "\\.(jsx|js|ts|tsx)$",
		"excludePattern": "(node_modules|docs)"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"destination": "./docs",
		"recurse": true,
		"template": "node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"title": "PasuNashi",
			"theme": "dark",
			"menu": [
				{
					"title": "PasuNashi Web",
					"link": "https://pasunashi.xyz",
					"target": "_blank"
				},
				{
					"title": "GitHub",
					"link": "https://github.com/PasuSewa",
					"target": "_blank"
				}
			]
		},
		"readme": "./README.md"
	},
	"tags": {
		"allowUnknownTags": ["category", "component"]
	},
	"plugins": ["node_modules/better-docs/typescript"]
}
